<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="org.hns.bean.User" table="userinfo">
  <id column="userId" name="userId" type="int">
   <generator class="increment"/>
  </id>
  <property column="sinaId" generated="never" lazy="false" name="sinaId" type="int"/>
  <property column="username" generated="never" lazy="false"
   name="username" type="string"/>
  <property column="usergender" generated="never" lazy="false"
   name="usergender" type="string"/>
  <property column="password" name="password" type="string"/>
 </class>
</hibernate-mapping>
